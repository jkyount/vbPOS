<Window x:Class="MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:POS1"
        mc:Ignorable="d"
        x:Name="winMain"
        d:Height="750" 
        d:Width="1199" Background="#FFFFC357" WindowStartupLocation="CenterScreen" ScrollViewer.VerticalScrollBarVisibility="Disabled" WindowStyle="None"
        Title="Home" FontFamily="Calibri" FontSize="18" ResizeMode="NoResize" WindowState="Maximized">

    <Window.Resources>
        <DataTemplate DataType="{x:Type local:vmHomeBase}">
            <local:HomeBase/>
        </DataTemplate>
 
    </Window.Resources>
    
    <Grid>
        <Frame Content="{Binding View.ViewModel}">
        </Frame>
        



    </Grid>

</Window>
